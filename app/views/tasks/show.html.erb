<h2><%= t('view.detail') %></h2>
<p><%= t('view.title') %>: <%= @task.title %></p>
<p><%= t('view.content') %>: <%= @task.content %></p>
<p><%= t('view.end_date') %>: <%= @task.end_date %></p>
<p><%= t('view.start_status') %>: <%= @task.start_status %></p>
<p><%= t('view.priority') %>: <%= @task.priority %></p>
<% @task.labels.each do |label| %>
    <p><%= t('view.label') %> : <%= label.name %></p>
<% end %>

<%= link_to t('view.task_list'), tasks_path %>